#include<unordered_map>
int findUnique(int *arr, int n) {
    // Write your code here
    unordered_map<int, int> a;
    for(int i = 0; i < n; i++){
        a[arr[i]] = a[arr[i]]+1;
    }
    unordered_map<int,int> :: iterator it;
    for(it = a.begin(); it != a.end(); it++){
        if(it->second == 1){
            return it->first;
        }
    }
}
